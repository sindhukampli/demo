pipeline{
	agent any
	parameters {
		choice(choices: ['DEV', 'QA', 'PROD'],description: 'choose from above')
	}
	triggers {
		githubPush()
	}
	stages {
		stage('Checkout') {
			steps {
        echo "Checkout completed"
			}
		}
stage('Build') {
      			when {
        			branch "main"
      			}
			steps {
				sh 'echo "Building code"'
			}
		}
		stage('Test') {
			steps {
				echo "Static test"
			}
		}

		stage('Deploy') {
			steps {
				echo "Deploying"
			}
		}

	}
}
